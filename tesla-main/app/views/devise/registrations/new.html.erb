 <%#=render "home/header2"%>
<section class="flex-shrink-0">
  <div class="container-fluid">
    <div class="container pt-4 pb-7 ">
      <div class="row justify-content-center">
        <div class="col-12 col-xs-12 col-sm-10 col-md-8 col-lg-6 col-xl-4 col-xxl-4 ">
          <div class="row">
            <div class="col-12  ">
  
  <h1 class="col-dark-blue f-m-1 ">Create an Account</h1>
    <p class="f-l-4-regular  m-0 text-black pt-3 pb-4">Already have an account? <span class="col-primary f-l-4-bold"><a class="col-primary" href="/users/sign_in">Log in</a></span></p>
    <div class="mb-3">

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "devise/shared/error_messages", resource: resource %>

<div class="mb-3">
  <div class="field">
    <%= f.label :Email_address, class:"form-label-box pb-2" %><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email", class: "form-control-box f-l-primary-regular" %>
  </div>
<div class="mb-3">
  <div class="field">
    <%= f.label :password, class:"form-label-box pb-2" %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "new-password", class: "form-control-box f-l-primary-regular" %>
  </div>
<div class="mb-3">

    <%= f.label :Re_type_Password, class:"form-label-box pb-2" %><br />
    <%= f.password_field :password_confirmation, autocomplete: "new-password", class: "form-control-box f-l-primary-regular" %>
  </div>
  <div class="form-check pt-4 pb-4">
  <input class="form-check-input form-check-input-primary" type="checkbox" checked value="" id="flexCheckDefault">
  <label class="form-check-label f-l-secondary-regular col-gray" for="flexCheckDefault">
    By clicking Create Flux Account you agree to our <span class="f-l-secondary-bold"><a class="col-primary" href="#">Terms of Use</a></span> & <span class="f-l-secondary-bold"><a class="col-primary" href="#">Privacy Policy</a></span>
  </label>
</div>
<div class="d-grid gap-2">

    <%= f.submit "Create Account", class:"btn btn-primary-flux  sh-filter-button f-l-button-bold sh-filter-button   "   %>
  </div>
 
<% end %>

<%= render "devise/shared/links" %>

</div>
</div>
</div>
</div>
</div>
</div>
</div>
</section>